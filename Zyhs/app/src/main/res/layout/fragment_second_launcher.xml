<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@mipmap/ic_guide_second_bg">

    <ImageView
        android:id="@+id/iv_launcher_des"
        android:layout_width="946px"
        android:layout_height="410px"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="346px"
        android:visibility="gone"
        tools:visibility="visible"
        android:background="@mipmap/ic_guide_second_des"/>

    <ImageView
        android:id="@+id/iv_launcher_next"
        android:layout_width="253px"
        android:layout_height="104px"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:visibility="gone"
        android:layout_marginBottom="290px"
        android:layout_marginLeft="259px"
        tools:visibility="visible"
        android:background="@mipmap/ic_guide_next"/>


</android.support.constraint.ConstraintLayout>