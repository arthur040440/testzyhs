<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:showIn="@layout/dialog_throw">

    <ImageView
        android:id="@+id/iv_plastic_glove"
        android:layout_width="66px"
        android:layout_height="97px"
        android:layout_marginLeft="151px"
        android:layout_marginTop="457px"
        android:src="@mipmap/ic_type_plastic1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.tlh.android.widget.NoPaddingTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="134px"
        android:layout_marginTop="20px"
        android:text="@string/plastic_glove"
        android:textColor="@color/black"
        android:textSize="24px"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_plastic_glove" />

    <ImageView
        android:id="@+id/iv_plastic_packaging"
        android:layout_width="45px"
        android:layout_height="95px"
        android:src="@mipmap/ic_type_plastic2"
        app:layout_constraintBottom_toBottomOf="@id/iv_plastic_glove"
        android:layout_marginLeft="322px"
        app:layout_constraintLeft_toLeftOf="parent" />

    <com.tlh.android.widget.NoPaddingTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20px"
        android:text="@string/plastic_packaging"
        android:textColor="@color/black"
        android:textSize="24px"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="297px"
        app:layout_constraintTop_toBottomOf="@id/iv_plastic_packaging" />

    <ImageView
        android:id="@+id/iv_plastic_chair"
        android:layout_width="90px"
        android:layout_height="85px"
        android:layout_marginRight="300px"
        android:src="@mipmap/ic_type_plastic3"
        app:layout_constraintBottom_toBottomOf="@id/iv_plastic_glove"
        app:layout_constraintRight_toRightOf="parent" />

    <com.tlh.android.widget.NoPaddingTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20px"
        android:layout_marginRight="311px"
        android:text="@string/plastic_chair"
        android:textColor="@color/black"
        android:textSize="24px"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_plastic_chair" />

    <ImageView
        android:id="@+id/iv_plastic_basin"
        android:layout_width="106px"
        android:layout_height="80px"
        android:layout_marginRight="130px"
        android:src="@mipmap/ic_type_plastic4"
        app:layout_constraintBottom_toBottomOf="@id/iv_plastic_glove"
        app:layout_constraintRight_toRightOf="parent" />

    <com.tlh.android.widget.NoPaddingTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20px"
        android:layout_marginRight="147px"
        android:text="@string/plastic_basin"
        android:textColor="@color/black"
        android:textSize="24px"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_plastic_basin" />

</merge>