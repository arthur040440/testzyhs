<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="885px"
    android:layout_height="1447px"
    android:layout_gravity="center_horizontal"
    android:layout_marginTop="190px"
    android:layout_marginBottom="283px"
    android:background="@mipmap/coupons_zyhs_bg">


    <ImageView
        android:id="@+id/iv_coupons_zyhs_code"
        android:layout_width="261px"
        android:layout_height="261px"
        android:layout_marginTop="820px"
        android:layout_marginBottom="366px"
        android:src="@mipmap/coupons_zyhs_code"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <com.tlh.android.widget.NoPaddingTextView
        android:id="@+id/tv_throw_coupons_zyhs"
        android:layout_width="734px"
        android:layout_height="114px"
        android:layout_marginBottom="92px"
        android:background="@drawable/sec_btn_theme_bg"
        android:gravity="center"
        android:text="确定（45）"
        android:textColor="@color/tv_theme_color"
        android:textSize="38px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />


</android.support.constraint.ConstraintLayout>